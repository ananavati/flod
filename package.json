{
  "name": "flod",
  "description": "Node.js Web Framework benchmarking tool",
  "preferGlobal": true,
  "version": "0.0.7",
  "author": "Van Nguyen <thegoleffect@gmail.com>",
  "repository": "git://github.com/walmartlabs/flod",
  "engines": {
    "node": "0.8.x"
  },
  "dependencies": {
    "async": "0.1.x",
    "cli-table": "0.2.x",
    "colors": "0.6.x",
    "docopt": "0.4.x",
    "hapi": "0.14.x",
    "joi": "0.0.x",
    "node-uuid": "1.3.x",
    "request": "2.11.x",
    "underscore": "1.4.x"
  },
  "devDependencies": {
    "mocha": "1.x.x",
    "chai": "1.x.x",
    "blanket": "1.0.x",
    "travis-cov": "0.2.x"
  },
  "bin": {
    "flod": "./bin/flod",
    "flodd": "./bin/flodd"
  },
  "scripts": {
    "test": "make test && make test-cov",
    "blanket": { "pattern": "//^((?!\/node_modules\/)(?!\/test\/).)*$/ig", "onlyCwd": true, "data-cover-flags": { "branchTracking": true } },
    "travis-cov": { "threshold": 100 }
  },
  "licenses": [
    {
      "type": "BSD",
      "url": "http://github.com/walmartlabs/flod/raw/master/LICENSE"
    }
  ]
}
